    "otplib": "^12.0.1",
    "bcryptjs": "^2.4.3",
    "fastify": "^5.2.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
